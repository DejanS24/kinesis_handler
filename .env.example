#######################
# Not used with the current implementation

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Kinesis Configuration
KINESIS_STREAM_NAME=my-kinesis-stream
KINESIS_STREAM_ARN=arn:aws:kinesis:us-east-1:123456789012:stream/my-kinesis-stream

# Lambda Configuration (if applicable)
LAMBDA_FUNCTION_NAME=kinesis-handler
LAMBDA_TIMEOUT=60
LAMBDA_MEMORY_SIZE=256

#######################

# Application Configuration
LOG_LEVEL=info

# Repository Configuration
# Options: 'inmemory' | 'dynamodb'
REPOSITORY_TYPE=inmemory

DYNAMODB_ENDPOINT = 'http://localhost:8000';

# DynamoDB Configuration (required if REPOSITORY_TYPE=dynamodb)
USER_LIMIT_TABLE_NAME=UserLimits

# Dead Letter Queue (DLQ) Configuration
# Note: DLQ setup must be done through AWS Console or IaC (Terraform/CloudFormation)
# Set event source mapping configuration:
# - MaximumRetryAttempts: 2
# - OnFailure: arn:aws:sqs:REGION:ACCOUNT:kinesis-handler-dlq
# - BisectBatchOnFunctionError: true
